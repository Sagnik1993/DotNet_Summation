using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        int[] array1 = { 1, 3, 5, 7 };
        List<int> list1 = new List<int>() { 1, 3, 5, 7 };
        int sum1 = array1.Sum();
        int sum2 = list1.Sum();
        Console.WriteLine(sum1);
        Console.WriteLine(sum2);
    }
}
